{
	"name": "console-logger",
	"displayName": "console logger",
	"description": "To create console log in JavaScript, and customized for userself.",
	"repository": {
		"type": "git",
		"url":"https://github.com/gitshan/vscode-extension-console-logger.git"
	},
	"version": "1.0.0",
	"publisher": "huangbaoshan",
	"icon": "icon/icon.jpg",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.consoleLogger"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.consoleLogger",
			"title": "Console Logger"
		}],
		"keybindings": [{
			"key": "ctrl+alt+q",
			"mac": "ctrl+alt+q",
			"command": "extension.consoleLogger"
		}],
        "configuration": {
            "type": "object",
            "title": "vscode-plugin-console-logger",
            "properties": {
                "vscodePluginConsoleLogger.identifier": {
                    "type": "string",
                    "default": ": ",
                    "description": "描述与值的分隔符，默认: "
                }
            }
        }
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
